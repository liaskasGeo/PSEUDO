акцояихлос amintikos
дедолема
    X,Y,Z,W:INTEGER;

аявг
    тупысе("дЧСЕ ТОУР АЯИХЛОЩР в,у,ф,W: ",EOLN)
    
    епамакабе
        тупысе(EOLN,"дЧСЕ ТО X: ")
        диабасе(X)
    левяи (X>=5 AND X<=10)

    епамакабе
        тупысе("дЧСЕ ТО Y: ")
        диабасе(Y)
    левяи (Y>0)

    епамакабе
        тупысе("дЧСЕ ТО Z: ")
        диабасе(Z)
    левяи (Z>=0)

    епамакабе
        тупысе("дЧСЕ ТО W: ")
        диабасе(W)
    левяи (W<0)

    тупысе(EOLN,"еУВАЯИСТОЩЛЕ")
текос